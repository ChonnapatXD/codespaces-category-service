generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_WRITE_URL")
}

model Menu {
  id       Int      @id @default(autoincrement())
  name     String
  price    Int
  image    String
  category Category
}

enum Category {
  main
  snack
  drink
}
